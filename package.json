{
  "name": "enum-xyz",
  "type": "module",
  "version": "0.0.6",
  "description": "JavaScript enums using proxies.",
  "homepage": "https://github.com/chasefleming/enum-xyz",
  "author": "Chase Fleming",
  "keywords": [
    "enum",
    "proxy"
  ],
  "source": "src/index.js",
  "exports": {
    "require": "./dist/enum-xyz.cjs",
    "default": "./dist/enum-xyz.modern.js"
  },
  "main": "dist/enum-xyz.cjs",
  "module": "dist/enum-xyz.module.js",
  "unpkg": "dist/enum-xyz.umd.js",
  "types": "dist/enum-xyz.d.ts",
  "scripts": {
    "dev": "microbundle watch",
    "format": "prettier --write **/*.{js,json,md} --ignore-path .gitignore",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "build": "rm -rf dist && microbundle",
    "publish": "npm run build && npm publish"
  },
  "license": "ISC",
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "jest": "^27.5.1",
    "microbundle": "^0.14.2",
    "prettier": "^2.5.1"
  }
}
